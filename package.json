{
  "name": "cardify",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "enzyme": "^3.3.0",
    "node-sass-chokidar": "^1.2.2",
    "prop-types": "^15.6.1",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.3.2",
    "workbox-cli": "^3.1.0"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && workbox injectManifest workbox-config.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/Components -o src/Components",
    "watch-css": "npm run build-css && node-sass-chokidar src/Components -o src/Components --watch --recursive"
  }
}
